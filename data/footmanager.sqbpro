CREATE TABLE IF NOT EXISTS equipos (
    id INTEGER PRIMARY KEY,
    nombre TEXT NOT NULL,
    liga_id INTEGER,
    ciudad TEXT,
    FOREIGN KEY (liga_id) REFERENCES ligas(id)
);

CREATE TABLE IF NOT EXISTS jugadores (
    id INTEGER PRIMARY KEY,
    nombre TEXT NOT NULL,
    edad INTEGER,
    posicion TEXT,
    equipo_id INTEGER,
    FOREIGN KEY (equipo_id) REFERENCES equipos(id)
);

CREATE TABLE IF NOT EXISTS atributos_jugador (
    jugador_id INTEGER PRIMARY KEY,
    remate INTEGER, pase INTEGER, tecnica INTEGER, desmarques INTEGER, tiros_lejanos INTEGER,
    marcaje INTEGER, entradas INTEGER, colocacion INTEGER, vision INTEGER, agresividad INTEGER,
    agarre INTEGER, despeje INTEGER, mando_area INTEGER,
    FOREIGN KEY (jugador_id) REFERENCES jugadores(id)
);
